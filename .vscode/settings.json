{
  "files.eol": "\n",
  "css.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.validate": true,
  "tailwindCSS.experimental.classRegex": [
    // "classNames\\(\"(.*)\"\\)",
    "Classes\\s*=\\s*\"([^\"]*)\"",
    ".+ClassName\\s*=\\s*\"([^\"]*)\"",
    "const\\s+.*ClassName\\s*=\\s*\"([^']*)\""
  ],
  "tailwindCSS.lint.cssConflict": "ignore",
  "files.exclude": {
    "**/dist": true,
    "**/node_modules": true
  }
}
