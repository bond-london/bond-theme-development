{
  "extends": "nx/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "parallel": false,
        "cacheableOperations": [
          "format",
          "develop",
          "build",
          "clean",
          "rebuild",
          "typecheck",
          "lint",
          "build:browser",
          "watch",
          "watch:browser",
          "prepare",
          "build:gatsby-config",
          "watch:gatsby-config",
          "build:gatsby-node",
          "watch:gatsby-node",
          "build:gatsby-ssr",
          "watch:gatsby-ssr",
          "xbuild:browser",
          "xwatch:browser"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "prepare": {
      "dependsOn": ["^prepare"]
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
