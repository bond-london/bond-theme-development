{
  "name": "@bond-london/graphcms-rich-text",
  "sideEffects": false,
  "version": "2.0.0-pre.1",
  "description": "Rich text components",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "gatsby",
    "graphcms",
    "rich-text"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/bond-graphcms-rich-text"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "typegen": "tsc --emitDeclarationOnly --declaration --declarationDir dist/",
    "xbuild:all": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "build:tsc": "tsc --outDir dist/",
    "build": "npm-run-all -s clean build:tsc",
    "watch": "npm-run-all -s watch:tsc",
    "xwatch:all": "yarn build:all -w",
    "watch:tsc": "yarn build:tsc -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@graphcms/rich-text-types": "^0.5.0",
    "babel-preset-gatsby-package": "^2.21.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "react": "^18.x"
  }
}
