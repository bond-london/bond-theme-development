{
  "name": "@bond-london/gatsby-theme",
  "version": "1.0.0-pre.1",
  "description": "Transform video files",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-theme"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.modern.js",
  "browser": {
    "dist/index.js": "dist/index.brower.js",
    "dist/index.module.js": "dist/index.browser.modern.js"
  },
  "files": [
    "gatsby-config.js",
    "gatsby-env.js",
    "dist"
  ],
  "scripts": {
    "build:gatsby-config": "tsc -p tsconfig.gatsby-config.json --outDir dist",
    "build:server": "microbundle -i src/index.ts -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment",
    "build:browser": "microbundle -i src/index.ts -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment -o dist/index.browser",
    "build": "npm-run-all -s clean -p build:*",
    "watch": "npm-run-all --npm-path npm -s clean -p watch:*",
    "watch:gatsby-config": "yarn build:gatsby-node --watch",
    "watch:server": "yarn build:server --no-compress watch",
    "watch:browser": "yarn build:browser --no-compress watch",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "clean": "del-cli dist/*"
  },
  "peerDependencies": {
    "gatsby": "^4.20.0",
    "gatsby-plugin-image": "^2.21.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@bond-london/bond-tailwind-plugin": "^5.0.0-pre.1",
    "@bond-london/gatsby-graphcms-components": "^5.0.0-pre.1",
    "@bond-london/gatsby-transformer-extracted-lottie": "^4.0.0-pre.1",
    "@bond-london/gatsby-transformer-extracted-svg": "^3.0.0-pre.1",
    "@bond-london/gatsby-transformer-video": "^2.0.0-pre.1",
    "@bond-london/graphcms-rich-text": "^2.0.0-pre.1",
    "@bond-london/simple-gatsby-source-graphcms": "^7.0.0-pre.1",
    "classnames": "^2.3.1",
    "dotenv": "^16.0.1",
    "gatsby-plugin-sharp": "^4.21.0",
    "gatsby-source-filesystem": "^4.21.1",
    "gatsby-transformer-sharp": "^4.21.0"
  },
  "devDependencies": {
    "babel-preset-gatsby-package": "^2.21.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  }
}
