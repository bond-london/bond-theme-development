{
  "name": "@bond-london/simple-gatsby-source-graphcms",
  "sideEffects": false,
  "version": "7.0.0-pre.1",
  "description": "Bond version of the official Gatsby source plugin for GraphCMS projects",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "keywords": [
    "data",
    "gatsby",
    "gatsby-plugin",
    "graphcms",
    "graphql",
    "source"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/bond-source-graphcms"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "babel src --out-dir dist --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "watch": "babel -w src --out-dir dist --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "dependencies": {
    "@graphcms/rich-text-types": "^0.5.0",
    "async-sema": "^3.1.1",
    "fs-extra": "^10.0.1",
    "gatsby-graphql-source-toolkit": "^2.0.1",
    "gatsby-plugin-image": "^2.20.0",
    "gatsby-plugin-utils": "^3.14.0",
    "gatsby-source-filesystem": "^4.20.0",
    "node-fetch": "2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "babel-preset-gatsby-package": "^2.21.0",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0||^4.0.0"
  }
}
