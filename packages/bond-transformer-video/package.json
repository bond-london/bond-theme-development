{
  "name": "@bond-london/gatsby-transformer-video",
  "version": "2.0.0-pre.1",
  "description": "Transform video files",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.module.js",
  "files": [
    "gatsby-node.js",
    "gatsby-worker.js",
    "gatsby-ssr.js",
    "dist"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/bond-transformer-video"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:gatsby-node": "tsc -p tsconfig.gatsby-node.json --outDir dist",
    "build:gatsby-ssr": "microbundle -i src/gatsby-ssr.tsx -f cjs -o ./[name].js --no-pkg-main --jsx React.createElement --jsxFragment React.Fragment --no-compress --external=common-tags,react --no-sourcemap",
    "build:components": "microbundle -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment",
    "build": "npm-run-all -s clean -p build:*",
    "watch": "npm-run-all --npm-path npm -s clean -p watch:*",
    "watch:gatsby-node": "yarn build:gatsby-node --watch",
    "watch:gatsby-ssr": "yarn build:gatsby-ssr watch",
    "watch:components": "yarn build:components --no-compress watch",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "clean": "del-cli dist/*"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.11.0",
    "ffmpeg-static": "^5.0.2",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "sharp": "^0.30.7"
  },
  "peerDependencies": {
    "gatsby": "^4.x",
    "gatsby-cli": "^4.x",
    "gatsby-source-filesystem": "^4.x",
    "gatsby-worker": "^1.19.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/ffprobe-static": "^2.0.1",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^9.0.13",
    "@types/react": "^18.0.15",
    "@types/sharp": "^0.30.5",
    "babel-preset-gatsby-package": "^2.21.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  }
}
