    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "gatsby": "^4 || ^5"
  },
  "gitHead": "b992a2096451a0d196c81d502ae3f1981c96cc9f"
