{
  "name": "@bond-london/gatsby-plugin-cms-i18next",
  "version": "3.0.0-pre.1",
  "description": "Use CMS languages to generate your Gatsby website",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "react",
    "gatsby",
    "gatsbyjs",
    "gatsby-plugin",
    "gatsby-component",
    "i18next",
    "react-i18next",
    "gatsby-i18n",
    "i18n",
    "i18next",
    "localization",
    "localisation",
    "translation",
    "translate"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.modern.js",
  "browser": {
    "dist/index.js": "dist/index.brower.js",
    "dist/index.module.js": "dist/index.browser.modern.js"
  },
  "files": [
    "gatsby-node.js",
    "gatsby-worker.js",
    "dist"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/gatsby-plugin-cms-i18next"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:all": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "build": "npm-run-all -s clean build:all",
    "watch": "npm-run-all -s clean watch:all",
    "watch:all": "yarn build:all -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "devDependencies": {
    "@types/browser-lang": "^0.1.0",
    "@types/react": "^18.0.15",
    "@types/react-helmet": "^6.1.4",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "browser-lang": "^0.1.0",
    "i18next": "^21.8.13",
    "react-i18next": "^11.18.0",
    "i18next-browser-languagedetector": "^6.1.4"
  },
  "peerDependencies": {
    "gatsby": "^4",
    "i18next": "^21.x",
    "react": "^18"
  }
}
