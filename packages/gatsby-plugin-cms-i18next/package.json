{
  "name": "@bond-london/gatsby-plugin-cms-i18next",
  "version": "3.0.0-pre.1",
  "description": "Use CMS languages to generate your Gatsby website",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "react",
    "gatsby",
    "gatsbyjs",
    "gatsby-plugin",
    "gatsby-component",
    "i18next",
    "react-i18next",
    "gatsby-i18n",
    "i18n",
    "i18next",
    "localization",
    "localisation",
    "translation",
    "translate"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.modern.js",
  "browser": {
    "dist/index.js": "dist/index.brower.js",
    "dist/index.module.js": "dist/index.browser.modern.js"
  },
  "files": [
    "gatsby-node.js",
    "gatsby-worker.js",
    "dist"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/gatsby-plugin-cms-i18next"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:gatsby-node": "tsc -p tsconfig.gatsby-node.json --outDir dist",
    "build:gatsby-ssr": "microbundle -i src/gatsby-ssr.ts -f cjs -o ./dist/[name].js --no-pkg-main --jsx React.createElement --jsxFragment React.Fragment --no-compress --external=common-tags,react --no-sourcemap",
    "build:server": "microbundle -i src/index.ts -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment",
    "build:browser": "microbundle -i src/index.ts -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment -o dist/index.browser",
    "build": "npm-run-all -s clean -p build:*",
    "watch": "npm-run-all --npm-path npm -s clean -p watch:*",
    "watch:gatsby-node": "yarn build:gatsby-node --watch",
    "watch:gatsby-ssr": "yarn build:gatsby-ssr watch",
    "watch:server": "yarn build:server --no-compress watch",
    "watch:browser": "yarn build:browser --no-compress watch",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "clean": "del-cli dist/*"
  },
  "devDependencies": {
    "@types/browser-lang": "^0.1.0",
    "@types/react": "^18.0.15",
    "@types/react-helmet": "^6.1.4",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "browser-lang": "^0.1.0",
    "i18next": "^21.8.13",
    "react-i18next": "^11.18.0",
    "i18next-browser-languagedetector": "^6.1.4"
  },
  "peerDependencies": {
    "gatsby": "^4",
    "i18next": "^21.x",
    "react": "^18"
  }
}
