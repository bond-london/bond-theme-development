{
  "name": "@bond-london/gatsby-plugin-cms-i18next",
  "version": "4.7.0",
  "description": "Use CMS languages to generate your Gatsby website",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "react",
    "gatsby",
    "gatsbyjs",
    "gatsby-plugin",
    "gatsby-component",
    "i18next",
    "react-i18next",
    "gatsby-i18n",
    "i18n",
    "i18next",
    "localization",
    "localisation",
    "translation",
    "translate"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/gatsby-plugin-cms-i18next"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "src/index.ts",
  "files": [
    "gatsby-browser.ts",
    "gatsby-node.js",
    "gatsby-ssr.ts",
    "dist",
    "src"
  ],
  "scripts": {
    "build:gatsby-node": "tsc --downlevelIteration true --skipLibCheck true --esModuleInterop true --outDir dist/ src/gatsby-node.ts",
    "build:tsc": "tsc --noEmit",
    "build": "npm-run-all -s clean build:*",
    "watch": "npm-run-all -s watch:*",
    "watch:gatsby-node": "yarn build:gatsby-node -w",
    "watch:tsc": "yarn build:tsc -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "devDependencies": {
    "@types/browser-lang": "^0.1.0",
    "@types/react": "^18.0.15",
    "@types/react-helmet": "^6.1.4",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "browser-lang": "^0.2.0",
    "i18next": "^23.1.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "react-i18next": "^13.0.0"
  },
  "peerDependencies": {
    "gatsby": "^4 || ^5",
    "i18next": "^21.x"
  },
  "gitHead": "5c1e8033f5b218b11ccfec5b579df6b6a657449e"
}
