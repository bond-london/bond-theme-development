{
  "name": "@bond-london/gatsby-source-hubspot-forms",
  "sideEffects": false,
  "version": "2.1.0",
  "description": "Support hubspot forms inside gatsby",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "keywords": [
    "data",
    "gatsby",
    "gatsby-plugin",
    "hubspot",
    "graphql",
    "source"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/gatsby-source-hubspot-forms"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "src/index.ts",
  "files": [
    "dist",
    "gatsby-node.js"
  ],
  "scripts": {
    "build:tsc": "tsc --outDir dist/",
    "build": "npm-run-all -s clean build:*",
    "watch": "npm-run-all -s watch:*",
    "watch:tsc": "yarn build:tsc -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "dependencies": {
    "@bond-london/gatsby-theme": "^2.10.0",
    "axios": "^1.2.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "gatsby": "^4 || ^5",
    "gatsby-plugin-utils": "^3 || ^4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
