{
  "name": "@bond-london/gatsby-theme",
  "version": "2.0.0-alpha.1",
  "description": "Transform video files",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-theme"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/bond-london-gatsby-theme"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "src/index.ts",
  "files": [
    "gatsby-config.js",
    "gatsby-browser.ts",
    "dist",
    "src"
  ],
  "scripts": {
    "build:gatsby-node": "tsc --downlevelIteration true --skipLibCheck true --esModuleInterop true --outDir dist/ src/gatsby-config.ts",
    "build:tsc": "tsc --noEmit",
    "build": "npm-run-all -s clean build:*",
    "watch": "npm-run-all -s watch:*",
    "watch:gatsby-node": "yarn build:gatsby-node -w",
    "watch:tsc": "yarn build:tsc -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "peerDependencies": {
    "gatsby": "^4 || ^5"
  },
  "dependencies": {
    "@bond-london/bond-tailwind-plugin": "^3.0.0-alpha.1",
    "@bond-london/eslint-config-bond-gatsby": "^2.0.0-alpha.0",
    "@bond-london/gatsby-graphcms-components": "^6.0.0-alpha.1",
    "@bond-london/gatsby-transformer-extracted-lottie": "^5.0.0-alpha.1",
    "@bond-london/gatsby-transformer-extracted-svg": "^4.0.0-alpha.1",
    "@bond-london/gatsby-transformer-video": "^3.0.0-alpha.1",
    "@bond-london/graphcms-rich-text": "^3.0.0-alpha.1",
    "@bond-london/simple-gatsby-source-graphcms": "^8.0.0-alpha.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "dotenv": "^16.0.1",
    "eslint-webpack-plugin": "^3.2.0",
    "gatsby-plugin-canonical-urls": "next",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-image": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-page-creator": "next",
    "gatsby-plugin-postcss": "next",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-sitemap": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-sharp": "next",
    "no-case": "^3.0.4",
    "react": "experimental",
    "react-dom": "experimental",
    "react-player": "^2.10.1",
    "tailwindcss": "^3.1.8",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "^3.1.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5"
  },
  "gitHead": "5c1e8033f5b218b11ccfec5b579df6b6a657449e"
}
