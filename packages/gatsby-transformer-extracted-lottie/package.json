{
  "name": "@bond-london/gatsby-transformer-extracted-lottie",
  "sideEffects": false,
  "version": "5.0.0-alpha.0",
  "description": "Gatsby plugin to transform lottie files and extract a preview svg",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-typescript",
    "typescript",
    "lottie"
  ],
  "main": "src/index.ts",
  "files": [
    "gatsby-node.js",
    "gatsby-ssr.tsx",
    "dist",
    "src"
  ],
  "repository": {
    "url": "https://github.com/bond-london/gatsby-transformer-extracted-lottie"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:mb": "microbundle -f cjs,modern --jsx React.createElement",
    "build:defs": "tsc --jsx react -esModuleInterop true --emitDeclarationOnly --declaration true --declarationDir dist src/index.ts",
    "build:gatsby-node": "tsc --downlevelIteration true --skipLibCheck true --esModuleInterop true --outDir dist/ src/gatsby-node.ts",
    "build:tsc": "tsc --outDir dist/",
    "copy:lottie": "copyfiles lottie_svg.js dist/",
    "build": "npm-run-all -s clean copy:lottie build:gatsby-node build:mb build:defs",
    "postbuild": "prepend-directive --files=dist/index.js,dist/index.modern.mjs --directive=\"client export\"",
    "watch": "npm-run-all -s watch:tsc",
    "watch:tsc": "yarn build:tsc -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prepend-directive": "^1.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "jsdom": "^20.0.0",
    "mini-svg-data-uri": "^1.4.4",
    "svgo": "^3.0.0"
  },
  "peerDependencies": {
    "gatsby": "^4 || ^5",
    "gatsby-source-filesystem": "^4 || ^5"
  },
  "gitHead": "5c1e8033f5b218b11ccfec5b579df6b6a657449e"
}
