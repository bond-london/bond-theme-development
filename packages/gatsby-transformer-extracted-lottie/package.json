{
  "name": "@bond-london/gatsby-transformer-extracted-lottie",
  "sideEffects": false,
  "version": "4.0.0-pre.1",
  "description": "Gatsby plugin to transform lottie files and extract a preview svg",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-typescript",
    "typescript",
    "lottie"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.modern.js",
  "browser": {
    "dist/index.js": "dist/index.brower.js",
    "dist/index.module.js": "dist/index.browser.modern.js"
  },
  "files": [
    "gatsby-node.js",
    "dist"
  ],
  "repository": {
    "url": "https://github.com/bond-london/gatsby-transformer-extracted-lottie"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:gatsby-node": "tsc -p tsconfig.gatsby-node.json --outDir dist",
    "build:server": "microbundle -i src/index.ts -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment",
    "build:browser": "microbundle -i src/index.ts -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment -o dist/index.browser",
    "build": "npm-run-all -s clean -p build:*",
    "watch": "npm-run-all --npm-path npm -s clean -p watch:*",
    "watch:gatsby-node": "yarn build:gatsby-node --watch",
    "watch:server": "yarn build:server --no-compress watch",
    "watch:browser": "yarn build:browser --no-compress watch",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "clean": "del-cli dist/*"
  },
  "devDependencies": {
    "@types/svgo": "^2.6.3",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "lottie-to-svg": "^1.0.0",
    "mini-svg-data-uri": "^1.4.4",
    "svgo": "^2.8.0"
  },
  "peerDependencies": {
    "gatsby": "^4",
    "gatsby-source-filesystem": "^4"
  }
}
