{
  "name": "@bond-london/gatsby-transformer-extracted-lottie",
  "sideEffects": false,
  "version": "4.0.0-pre.1",
  "description": "Gatsby plugin to transform lottie files and extract a preview svg",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-typescript",
    "typescript",
    "lottie"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "gatsby-node.js",
    "gatsby-ssr.js",
    "dist"
  ],
  "repository": {
    "url": "https://github.com/bond-london/gatsby-transformer-extracted-lottie"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:all": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "build": "npm-run-all -s clean build:all",
    "watch": "npm-run-all -s clean watch:all",
    "watch:all": "yarn build:all -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "devDependencies": {
    "@types/svgo": "^2.6.3",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "lottie-to-svg": "^1.0.0",
    "lottie-web": "^5.9.6",
    "mini-svg-data-uri": "^1.4.4",
    "svgo": "^2.8.0"
  },
  "peerDependencies": {
    "gatsby": "^4",
    "gatsby-source-filesystem": "^4"
  }
}
