{
  "name": "@bond-london/gatsby-transformer-extracted-svg",
  "version": "3.0.0-pre.1",
  "description": "Extract SVG from SVG files",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-typescript",
    "typescript",
    "svg"
  ],
  "xmain": "dist/index.js",
  "xtypes": "dist/index.d.ts",
  "xmodule": "dist/index.module.js",
  "xesmodule": "dist/index.modern.js",
  "xbrowser": {
    "dist/index.js": "dist/index.brower.js",
    "dist/index.module.js": "dist/index.browser.modern.js"
  },
  "files": [
    "gatsby-node.js",
    "dist"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/gatsby-transformer-extracted-svg"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:gatsby-node": "tsc -p tsconfig.gatsby-node.json --outDir dist",
    "xbuild:server": "microbundle -i src/index.ts -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment",
    "xbuild:browser": "microbundle -i src/index.ts -f cjs,modern --jsx React.createElement --jsxFragment React.Fragment -o dist/index.browser",
    "build": "npm-run-all -s clean -p build:*",
    "watch": "npm-run-all --npm-path npm -s clean -p watch:*",
    "watch:gatsby-node": "yarn build:gatsby-node --watch",
    "xwatch:server": "yarn build:server --no-compress watch",
    "xwatch:browser": "yarn build:browser --no-compress watch",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "clean": "del-cli dist/*"
  },
  "dependencies": {
    "mini-svg-data-uri": "^1.4.3",
    "svgo": "^2.8.0"
  },
  "devDependencies": {
    "@types/svgo": "^2.6.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "gatsby": "^4.x",
    "gatsby-source-filesystem": "^4"
  }
}
