{
  "name": "@bond-london/gatsby-transformer-extracted-svg",
  "version": "3.0.0-pre.5",
  "description": "Extract SVG from SVG files",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-typescript",
    "typescript",
    "svg"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/gatsby-transformer-extracted-svg"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "gatsby-node.js",
    "gatsby-ssr.tsx",
    "dist",
    "src/fragments.ts"
  ],
  "scripts": {
    "build:tsc": "tsc --outDir dist/",
    "build": "npm-run-all -s clean build:tsc",
    "watch": "npm-run-all -s watch:tsc",
    "watch:tsc": "yarn build:tsc -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "dependencies": {
    "mini-svg-data-uri": "^1.4.3",
    "svgo": "^2.8.0"
  },
  "devDependencies": {
    "@types/svgo": "^2.6.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "gatsby": "^4.x",
    "gatsby-source-filesystem": "^4"
  },
  "gitHead": "5c1e8033f5b218b11ccfec5b579df6b6a657449e"
}
