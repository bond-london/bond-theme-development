{
  "name": "@bond-london/gatsby-transformer-video",
  "version": "3.2.0",
  "description": "Transform video files",
  "author": "Nick Randell <nick.randell@bond.fi>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "repository": {
    "url": "https://github.com/bond-london/bond-london-theme",
    "directory": "packages/bond-transformer-video"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./src/index.ts",
  "files": [
    "gatsby-node.js",
    "gatsby-worker.js",
    "gatsby-ssr.tsx",
    "dist",
    "src"
  ],
  "scripts": {
    "build:gatsby-node": "tsc --downlevelIteration true --skipLibCheck true --esModuleInterop true --outDir dist/ src/gatsby-node.ts src/gatsby-worker.ts",
    "build:tsc": "tsc --noEmit",
    "build": "npm-run-all -s clean build:*",
    "watch": "npm-run-all -s watch:*",
    "watch:gatsby-node": "yarn build:gatsby-node -w",
    "watch:tsc": "yarn build:tsc -w",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "clean": "del-cli dist/*"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.11.0",
    "ffmpeg-static": "^5.0.2",
    "fluent-ffmpeg": "^2.1.2",
    "sharp": "^0.31.0"
  },
  "peerDependencies": {
    "gatsby": "^4 || ^5",
    "gatsby-cli": "^4 || ^5",
    "gatsby-source-filesystem": "^4 || ^5",
    "gatsby-worker": "^1 || ^2"
  },
  "devDependencies": {
    "@types/ffmpeg-static": "^3.0.1",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^9.0.13",
    "@types/react": "^18.0.15",
    "@types/sharp": "^0.31.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  },
  "gitHead": "5cf085faed0b43aa89f840d97b1400a435ff901e"
}
