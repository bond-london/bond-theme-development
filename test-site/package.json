{
  "name": "test-site",
  "private": "true",
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "version": "2.0.0",
  "description": "test-site",
  "author": "nrandell",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build:gatsby": "gatsby build",
    "build": "run-s clean build:gatsby",
    "watch": "develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "restart": "run-s clean develop",
    "rebuild": "run-s clean build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --rulesdir ../node_modules/gatsby/dist/utils/eslint-rules --config .custom.eslintrc.json --max-warnings 0 \"src/**/*.{js,jsx,ts,tsx}\" \"gatsby-*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "css": "tailwindcss -i ./src/styles/index.css -o built.css"
  },
  "dependencies": {
    "@bond-london/eslint-config-bond-gatsby": "^2.1.0",
    "@bond-london/gatsby-theme": "^2.0.0",
    "@bond-london/gatsby-transformer-extracted-lottie": "^5.0.2",
    "@bond-london/gatsby-transformer-extracted-svg": "^4.0.1",
    "@bond-london/gatsby-transformer-video": "^3.0.2",
    "@bond-london/graphcms-rich-text": "^3.0.1",
    "@sindresorhus/slugify": "^2.1.1",
    "classnames": "^2.3.1",
    "gatsby": "^5.0.0",
    "gatsby-cli": "^5.3.1",
    "gatsby-plugin-image": "^3.2.0",
    "pure-react-carousel": "^1.30.1",
    "react": "18",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "18"
  },
  "devDependencies": {
    "@graphcms/rich-text-types": "^0.5.0",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.6",
    "dotenv": "^16.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5"
  }
}
