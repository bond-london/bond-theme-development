{
  "name": "test-site",
  "private": "true",
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "version": "1.0.0",
  "description": "test-site",
  "author": "nrandell",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build:gatsby": "gatsby build --verbose",
    "build": "run-s clean build:gatsby",
    "watch": "develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "restart": "run-s clean develop",
    "rebuild": "run-s clean build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --rulesdir ../node_modules/gatsby/dist/utils/eslint-rules --config .custom.eslintrc.json --max-warnings 0 \"src/**/*.{js,jsx,ts,tsx}\" \"gatsby-*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@bond-london/gatsby-theme": "^1.0.0-pre.1",
    "classnames": "^2.3.1",
    "gatsby": "^4.20.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/node": "17",
    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.6",
    "dotenv": "^16.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5"
  }
}
