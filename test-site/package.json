{
  "name": "test-site",
  "private": "true",
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "version": "1.0.0",
  "description": "test-site",
  "author": "nrandell",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build --verbose",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "restart": "run-s clean develop",
    "rebuild": "run-s clean build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --rulesdir node_modules/gatsby/dist/utils/eslint-rules --config .custom.eslintrc.json --max-warnings 0 \"src/**/*.{js,jsx,ts,tsx}\" \"gatsby-*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@bond-london/gatsby-theme": "^1.0.0-pre.1",
    "classnames": "^2.3.1",
    "gatsby": "^4.20.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gatsby-plugin-eslint": "^4.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^4.7.4"
  }
}
